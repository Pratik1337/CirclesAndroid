<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TempLandingPage">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="94dp"
        android:text="Temporary Home Page"
        android:textSize="24dp"
        android:textStyle="italic"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <Button
        android:id="@+id/bt_view_profile"
        android:layout_width="203dp"
        android:layout_height="66dp"
        android:layout_marginTop="88dp"
        android:text="View My Profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/bt_temp_circle_home"
        android:layout_width="199dp"
        android:layout_height="66dp"
        android:layout_marginTop="36dp"
        android:text="Circles Home"
        app:layout_constraintEnd_toEndOf="@+id/bt_view_profile"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_view_profile"
        app:layout_constraintTop_toBottomOf="@+id/bt_view_profile" />

    <Button
        android:id="@+id/bt_temp_upload_post"
        android:layout_width="199dp"
        android:layout_height="66dp"
        android:layout_marginTop="36dp"
        android:text="Upload a post"
        app:layout_constraintEnd_toEndOf="@+id/bt_temp_circle_home"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_temp_circle_home"
        app:layout_constraintTop_toBottomOf="@+id/bt_temp_circle_home" />

    <Button
        android:id="@+id/bt_temp_logout"
        android:layout_width="199dp"
        android:layout_height="66dp"
        android:layout_marginTop="120dp"
        android:text="Log Out"
        app:layout_constraintEnd_toEndOf="@+id/bt_temp_upload_post"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="@+id/bt_temp_upload_post"
        app:layout_constraintTop_toBottomOf="@+id/bt_temp_upload_post" />

    <TextView
        android:id="@+id/tx_landing_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="6dp"
        android:textSize="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bt_register_user"
        android:layout_width="199dp"
        android:layout_height="66dp"
        android:layout_marginTop="32dp"
        android:text="Register "
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_temp_upload_post" />

    <ProgressBar
        android:id="@+id/pb_temp_landing"
        style="?android:attr/progressBarStyle"
        android:layout_width="31dp"
        android:layout_height="27dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="273dp"
        android:layout_height="121dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.434"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/circles_written_logo_black" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="76dp"
        android:layout_height="58dp"
        android:layout_marginTop="72dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:srcCompat="@drawable/blue_written" />
</androidx.constraintlayout.widget.ConstraintLayout>